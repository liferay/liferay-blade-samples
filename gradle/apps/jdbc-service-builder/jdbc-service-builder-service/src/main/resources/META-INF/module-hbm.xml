<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.blade.samples.jdbcservicebuilder.model.Country" />
	<class name="com.liferay.blade.samples.jdbcservicebuilder.model.impl.CountryImpl" table="country">
		<id column="id" name="countryId" type="long">
			<generator class="assigned" />
		</id>
		<property column="name" name="countryName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>