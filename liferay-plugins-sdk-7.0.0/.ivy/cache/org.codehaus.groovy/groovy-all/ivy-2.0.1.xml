<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:m="http://ant.apache.org/ivy/maven">
	<info organisation="org.codehaus.groovy"
		module="groovy-all"
		revision="2.0.1"
		status="release"
		publication="20140805221303"
	>
		<license name="The Apache Software License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0.txt" />
		<description homepage="http://groovy.codehaus.org/">
		Groovy: A powerful, dynamic language for the JVM
		</description>
	</info>
	<configurations>
		<conf name="default" visibility="public" description="runtime dependencies and master artifact can be used with this conf" extends="runtime,master"/>
		<conf name="master" visibility="public" description="contains only the artifact published by this module itself, with no transitive dependencies"/>
		<conf name="compile" visibility="public" description="this is the default scope, used if none is specified. Compile dependencies are available in all classpaths."/>
		<conf name="provided" visibility="public" description="this is much like compile, but indicates you expect the JDK or a container to provide it. It is only available on the compilation classpath, and is not transitive."/>
		<conf name="runtime" visibility="public" description="this scope indicates that the dependency is not required for compilation, but is for execution. It is in the runtime and test classpaths, but not the compile classpath." extends="compile"/>
		<conf name="test" visibility="private" description="this scope indicates that the dependency is not required for normal use of the application, and is only available for the test compilation and execution phases." extends="runtime"/>
		<conf name="system" visibility="public" description="this scope is similar to provided except that you have to provide the JAR which contains it explicitly. The artifact is always available and is not looked up in a repository."/>
		<conf name="sources" visibility="public" description="this configuration contains the source artifact of this module, if any."/>
		<conf name="javadoc" visibility="public" description="this configuration contains the javadoc artifact of this module, if any."/>
		<conf name="optional" visibility="public" description="contains all optional dependencies"/>
	</configurations>
	<publications>
		<artifact name="groovy-all" type="jar" ext="jar" conf="master"/>
		<artifact name="groovy-all" type="source" ext="jar" conf="sources" m:classifier="sources"/>
		<artifact name="groovy-all" type="javadoc" ext="jar" conf="javadoc" m:classifier="javadoc"/>
	</publications>
	<dependencies>
		<dependency org="org.apache.ant" name="ant-antlr" rev="1.8.4" force="true" conf="optional->compile(*),provided(*),master(*)"/>
		<dependency org="org.apache.ant" name="ant" rev="1.8.4" force="true" conf="optional->compile(*),master(*)"/>
		<dependency org="org.apache.ant" name="ant-junit" rev="1.8.4" force="true" conf="optional->compile(*),provided(*),master(*)">
			<exclude org="junit" module="junit" name="*" type="*" ext="*" conf="" matcher="exact"/>
		</dependency>
		<dependency org="org.apache.ant" name="ant-launcher" rev="1.8.4" force="true" conf="optional->compile(*),provided(*),master(*)"/>
		<dependency org="commons-logging" name="commons-logging" rev="1.1.1" force="true" conf="optional->compile(*),master(*)"/>
		<dependency org="bsf" name="bsf" rev="2.4.0" force="true" conf="optional->compile(*),master(*)">
			<exclude org="commons-logging" module="commons-logging" name="*" type="*" ext="*" conf="" matcher="exact"/>
		</dependency>
		<dependency org="com.thoughtworks.qdox" name="qdox" rev="1.12" force="true" conf="optional->compile(*),master(*)"/>
		<dependency org="javax.servlet" name="jsp-api" rev="2.0" force="true" conf="optional->compile(*),provided(*),runtime(*),master(*)"/>
		<dependency org="javax.servlet" name="servlet-api" rev="2.4" force="true" conf="optional->compile(*),provided(*),runtime(*),master(*)"/>
		<dependency org="junit" name="junit" rev="4.10" force="true" conf="optional->compile(*),master(*)"/>
		<dependency org="org.testng" name="testng" rev="6.5.2" force="true" conf="optional->compile(*),provided(*),master(*)">
			<exclude org="org.yaml" module="snakeyaml" name="*" type="*" ext="*" conf="" matcher="exact"/>
			<exclude org="org.beanshell" module="bsh" name="*" type="*" ext="*" conf="" matcher="exact"/>
			<exclude org="org.hamcrest" module="hamcrest-core" name="*" type="*" ext="*" conf="" matcher="exact"/>
		</dependency>
		<dependency org="jline" name="jline" rev="1.0" force="true" conf="optional->compile(*),master(*)">
			<exclude org="junit" module="junit" name="*" type="*" ext="*" conf="" matcher="exact"/>
		</dependency>
		<dependency org="com.thoughtworks.xstream" name="xstream" rev="1.4.2" force="true" conf="optional->compile(*),master(*)">
			<exclude org="jmock" module="jmock" name="*" type="*" ext="*" conf="" matcher="exact"/>
			<exclude org="junit" module="junit" name="*" type="*" ext="*" conf="" matcher="exact"/>
			<exclude org="xpp3" module="xpp3_min" name="*" type="*" ext="*" conf="" matcher="exact"/>
		</dependency>
		<dependency org="org.codehaus.gpars" name="gpars" rev="1.0-beta-2" force="true" conf="optional->compile(*),provided(*),master(*)"/>
		<dependency org="org.fusesource.jansi" name="jansi" rev="1.6" force="true" conf="optional->compile(*),master(*)"/>
		<dependency org="org.apache.ivy" name="ivy" rev="2.2.0" force="true" conf="optional->compile(*),master(*)"/>
	</dependencies>
</ivy-module>
